{
    "version": 2,
    "functions": {
        "api/*.py": {
            "runtime": "vercel-python@3.11",
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "myforum/wsgi.py"
        }
    ]
}
