{
  "version": 2,
  "builds": [
    {
      "src": "myforum/wsgi.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.11" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myforum/wsgi.py"
    }
  ]
}
