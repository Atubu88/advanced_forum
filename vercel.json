{
  "version": 2,
  "builds": [
    {
      "src": "myforum/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "staticfiles/**",
      "use": "@vercel/static"
    }
  ],
  "functions": {
    "myforum/wsgi.py": {
      "runtime": "@vercel/python",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myforum/wsgi.py"
    }
  ]
}
