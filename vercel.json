{
  "version": 2,
  "builds": [
    {
      "src": "accounts/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "staticfiles/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "accounts/wsgi.py"
    }
  ],
  "functions": {
    "accounts/wsgi.py": {
      "runtime": "python@3.11",
      "maxDuration": 30
    }
  }
}
